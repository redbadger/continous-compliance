{
  "contributors": [
    "Anubha Singh <anubha.singh@red-badger.com>",
    "Dimple Patel <dimple.patel@red-badger.com>",
    "Pedro Martin <pedro.martin@red-badger.com>"
  ],
  "description": "GitHub Action that gather compliance evidence and store it in the cloud",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/parser": "^4.21.0",
    "@vercel/ncc": "^0.28.2",
    "eslint": "^7.23.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4",
    "webpack": "^5.30.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "main": "lib/index.js",
  "name": "continous-compliance",
  "repository": "git@github.com:redbadger/continous-compliance.git",
  "scripts": {
    "build-package": "yarn build && yarn package",
    "build": "yarn tsc",
    "package": "yarn ncc build --source-map --license licenses.txt",
    "prepare": "yarn husky install"
  },
  "version": "1.0.0",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0"
  }
}
