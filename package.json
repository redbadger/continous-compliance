{
  "contributors": [
    "Anubha Singh <anubha.singh@red-badger.com>",
    "Dimple Patel <dimple.patel@red-badger.com>",
    "Pedro Martin <pedro.martin@red-badger.com>"
  ],
  "description": "GitHub Action that gather compliance evidence and store it in the cloud",
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^5.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^18.6.1",
    "@typescript-eslint/parser": "^4.21.0",
    "@vercel/ncc": "^0.28.3",
    "eslint": "^7.25.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "js-yaml": "^4.1.0",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.30.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "main": "lib/index.js",
  "name": "continous-compliance",
  "repository": "git@github.com:redbadger/continous-compliance.git",
  "scripts": {
    "build-package": "yarn build && yarn package",
    "build": "yarn tsc",
    "package": "yarn ncc build --source-map --license licenses.txt --minify",
    "prepare": "yarn husky install",
    "test": "yarn jest",
    "delete-tag": "sh ./scripts/delete-tag.sh",
    "create-tag": "sh ./scripts/create-tag.sh"
  },
  "version": "1.0.0",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.1",
    "@actions/io": "^1.1.0",
    "@google-cloud/storage": "^5.8.3",
    "prettier": "^2.2.1"
  }
}
